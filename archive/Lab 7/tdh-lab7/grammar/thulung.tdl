; Type assigning empty mod list. Added to basic types for nouns, verbs and determiners.

non-mod-lex-item := lex-item &
  [ SYNSEM.LOCAL.CAT.HEAD.MOD < > ].

;;;;;;;;;;;;;;;;;;;;;;;;;
;;; Matrix Type Addenda
;;;;;;;;;;;;;;;;;;;;;;;;;

basic-head-comp-phrase :+ [ SYNSEM [ LIGHT -,
             LOCAL.CAT.MC #mc,
             SUBORDINATED #subord ],
    HEAD-DTR.SYNSEM [ LOCAL.CAT.MC #mc,
                      SUBORDINATED #subord ] ].

head :+ [ PRON bool,
    AUX bool,
    PRD bool,
    NMZ bool,
    POSSESSOR poss,
    FORM form,
    INIT bool ].

noun :+ [ CASE case ].

coord-phrase :+ [ SYNSEM [ LOCAL.CAT.HEAD.NMZ #nmz,
             SUBORDINATED #subord ],
    LCOORD-DTR.SYNSEM [ LOCAL.CAT.HEAD.NMZ #nmz,
                        SUBORDINATED #subord ],
    RCOORD-DTR.SYNSEM [ LOCAL.CAT.HEAD.NMZ #nmz,
                        SUBORDINATED #subord ] ].

unary-bottom-coord-rule :+ [ SYNSEM [ LOCAL.CAT.HEAD.NMZ #nmz,
             SUBORDINATED #subord ],
    ARGS < [ SYNSEM [ LOCAL.CAT.HEAD.NMZ #nmz,
                      SUBORDINATED #subord ] ] > ].

inflected :+ [ UNDERSPECIFIED-PATIENT-FLAG luk,
    1PI-PATIENT-FLAG luk,
    ROLE-POSITION-CLASS-FLAG luk,
    ASPECT-POSITION-CLASS-FLAG luk,
    1DI-PATIENT-FLAG luk,
    POSSESSIVE-POSITION-CLASS-FLAG luk,
    TRANSITIVE-VERB-FLAG luk,
    ADD-NEGATION-TO-VERB-FLAG luk,
    3SG-PATIENT-FLAG luk,
    1PE-PATIENT-FLAG luk,
    1SG-PATIENT-FLAG luk,
    MOOD-POSITION-CLASS-FLAG luk,
    1DE-PATIENT-FLAG luk,
    CASE-POSITION-CLASS-FLAG luk,
    INTRANSITIVE-VERB-FLAG luk ].

infl-satisfied :+ [ UNDERSPECIFIED-PATIENT-FLAG na-or-+,
    1PI-PATIENT-FLAG na-or-+,
    ROLE-POSITION-CLASS-FLAG na-or-+,
    ASPECT-POSITION-CLASS-FLAG na-or-+,
    1DI-PATIENT-FLAG na-or-+,
    POSSESSIVE-POSITION-CLASS-FLAG na-or-+,
    TRANSITIVE-VERB-FLAG na-or-+,
    ADD-NEGATION-TO-VERB-FLAG na-or-+,
    3SG-PATIENT-FLAG na-or-+,
    1PE-PATIENT-FLAG na-or-+,
    1SG-PATIENT-FLAG na-or-+,
    MOOD-POSITION-CLASS-FLAG na-or-+,
    1DE-PATIENT-FLAG na-or-+,
    CASE-POSITION-CLASS-FLAG na-or-+,
    INTRANSITIVE-VERB-FLAG na-or-+ ].

png :+ [ PERNUM pernum ].

cat :+ [ POSSESSUM poss ].

poss := *top* &
  [ POSS-AGR png ].

basic-bare-np-phrase :+ [ SYNSEM.LOCAL.CAT [ VAL.SPEC < >,
                       HEAD #head,
                       POSSESSUM #possessum ],
    HEAD-DTR.SYNSEM.LOCAL.CAT [ HEAD #head,
                                POSSESSUM #possessum & nonpossessive ] ].

basic-head-1st-comp-phrase :+ [ SYNSEM.LOCAL.CAT.POSSESSUM #poss,
    HEAD-DTR.SYNSEM.LOCAL.CAT.POSSESSUM #poss ].

basic-head-opt-subj-phrase :+ [ SYNSEM.SUBORDINATED #subord,
    HEAD-DTR.SYNSEM [ LOCAL.CAT.VAL.COMPS < >,
                      SUBORDINATED #subord ] ].

adj-head-scop-phrase :+ [ NON-HEAD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR nonpossessive,
                                    POSSESSUM nonpossessive ] ].

top-coord-rule :+ [ SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                       POSSESSUM #possessum ],
    RCOORD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                                  POSSESSUM #possessum ],
    LCOORD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                                  POSSESSUM #possessum ] ].

bottom-coord-phrase :+ [ SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                       POSSESSUM #possessum ],
    NONCONJ-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                                   POSSESSUM #possessum ] ].

basic-head-mod-phrase-simple :+ [ SYNSEM.LOCAL.CAT.MC #mc,
    HEAD-DTR.SYNSEM.LOCAL.CAT.MC #mc ].

canonical-synsem :+ [ SUBORDINATED xsubord ].

xsubord := *top*.

none := xsubord.

cond := xsubord.

basic-head-subj-phrase :+ [ SYNSEM.SUBORDINATED #subord,
    HEAD-DTR.SYNSEM.SUBORDINATED #subord ].

basic-head-opt-comp-phrase :+ [ SYNSEM.SUBORDINATED #subord,
    HEAD-DTR.SYNSEM.SUBORDINATED #subord ].

adj-head-phrase :+ [ SYNSEM.SUBORDINATED #subord,
    NON-HEAD-DTR.SYNSEM.SUBORDINATED #subord ].

head-adj-phrase :+ [ SYNSEM.SUBORDINATED #subord,
    NON-HEAD-DTR.SYNSEM.SUBORDINATED #subord ].

non-local-change-only-lex-rule :+ same-subordinated-lex-rule.

local-change-only-lex-rule :+ same-subordinated-lex-rule.

cont-change-only-lex-rule :+ same-subordinated-lex-rule.

cat-change-with-ccont-lex-rule :+ same-subordinated-lex-rule.

add-only-rule :+ same-subordinated-lex-rule.

head-adj-int-phrase :+ [ NON-HEAD-DTR.SYNSEM.NON-LOCAL.QUE.LIST < > ].

;;;;;;;;;;;;;;
;;; Features
;;;;;;;;;;;;;;

;;; Pernum

pernum := *top*.
du := pernum.
2nd := pernum.
1du := 1st & du.
1du_incl := 1du & 2du+3du+1du_incl.
1du_excl := 1du.
2du := 2nd & 2du+3du.
1pl := 1st & pl.
1pl_incl := 1pl.
1pl_excl := 1pl.
2pl := 2nd & pl & 3sg+2sg+2pl+2du+3du.
3pl := 3rd & pl & 3sg+2sg+2pl+3pl+2du+3du.
1sg := 1st & sg.
2sg := 2nd & 2sg+plain & polite+2sg & 3sg+2sg.
1polite := 1st & polite.
2polite := 2nd & polite & pl+2polite.
3polite := 3rd & polite & pl+3polite.
1plain := 1st & plain.
2plain := 2nd & plain.
3plain := 3rd & plain.
2sg+plain := pernum.
plain := 2sg+plain & 3sg+plain.
polite+2sg := pernum.
polite := polite+2sg & 3sg+polite.
3sg+plain := pernum.
3sg := 3sg+plain & 3sg+polite & 3sg+2sg & 3sg+3du.
1st+sg := pernum.
1st := 1st+sg.
sg := 1st+sg.
3sg+polite := pernum.
pl+3rd := pernum.
3rd := pl+3rd.
pl+2polite := pernum.
pl := pl+2polite & pl+3polite.
pl+3polite := pl+3rd.
3sg+2sg := sg & 3sg+2sg+2pl+2du+3du.
3du := 2du+3du & 3sg+3du.
3sg+2sg+2pl+3pl+2du+3du := pernum.
3sg+2sg+2pl+2du+3du := 3sg+2sg+2pl+3pl+2du+3du.
2du+3du := 3sg+2sg+2pl+2du+3du & 2du+3du+1du_incl.
3sg+3du := 3rd & 3sg+2sg+2pl+2du+3du.
2du+3du+1du_incl := du.

;;; Possession

possession := *top*.
possessive := poss.  ; supertype for possessive features
nonpossessive := poss.  ; type for nonpossessive
possessor := possessive.
possessum := possessive.
possessive-1 := possessive.  ; supertype for strategy 1
possessor-1 := possessor & possessive-1.
possessum-1 := possessum & possessive-1.

;;; Possession

possessive-pron-1 := possessive.  ; supertype for strategy pron-1
possessor-pron-1 := possessor & possessive-pron-1.
possessum-pron-1 := possessum & possessive-pron-1.

;;; Case

case := *top*.
real-case := case.  ; intermediate case type for all real cases
NOM := real-case.  ; NOM
DAT := real-case.  ; DAT
ABL := real-case.  ; ABL
GEN := real-case.  ; GEN
COM := real-case.  ; COM
lev := real-case.  ; levLOC
hiL := real-case.  ; hiLOC
loL := real-case.  ; loLOC
LOC := real-case.  ; LOC
ERG+INS := real-case.
ERG := ERG+INS.
INS := ERG+INS.

;;; Form

form := *top*.
nonfinite := form.
finite := form.

;;; Tense

nonpast := tense.
past := tense.

;;; Aspect

POT := aspect.
HAB := aspect.
DEF := aspect.
STA := aspect.
PON := aspect.
RES := aspect.
ITF := aspect.
APX := aspect.
no-aspect := aspect.

;;; Mood

IRR := mood.
IMP := mood.
OBL := mood.

;;;;;;;;;;;;;;;;;;;
;;; Lexical Types
;;;;;;;;;;;;;;;;;;;

;;; Nouns

noun-lex := basic-noun-lex & basic-non-wh-word-lex & non-local-none-lex-item & no-hcons-lex-item & non-mod-lex-item &
  [ ARG-ST < #spr >,
    SYNSEM.LOCAL.CAT [ VAL [ SPR < #spr &
                                   [ LOCAL.CAT.HEAD det ] >,
                             COMPS < >,
                             SUBJ < >,
                             SPEC < > ],
                       HEAD.NMZ - ] ].

no-spr-noun-lex := noun-lex &
  [ SYNSEM.LOCAL.CAT.VAL.SPR < [ OPT + ] > ].

; Nouns which cannot take specifiers mark their SPR requirement
; as OPT +.  Making the non-head daughter OPT - in this rule
; keeps such nouns out.
;
; Rules for building NPs.  Note that the Matrix uses SPR for
; the specifier of nouns and SUBJ for the subject (specifier) of verbs.

head-spec-phrase := basic-head-spec-phrase & head-final &
  [ NON-HEAD-DTR.SYNSEM.OPT - ].

COMMON-NOUNS-noun-lex := noun-lex & CASE-POSITION-CLASS-rule-dtr & POSSESSIVE-POSITION-CLASS-rule-dtr &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM 3rd,
    INFLECTED [ CASE-POSITION-CLASS-FLAG -,
                POSSESSIVE-POSITION-CLASS-FLAG - ] ].

2nd-person-dual-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 2du ] ].

2nd-person-singular-plain-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 2sg+plain ] ].

2nd-person-singular-polite-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM polite+2sg ] ].

3rd-person-singular-plain-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 3sg+plain ] ].

indefinite-pronoun-noun-lex := no-spr-noun-lex &
  [ SYNSEM.LOCAL.CAT.HEAD.PRON + ].

borrowed-from-Nepali-evidential-like-noun-noun-lex := no-spr-noun-lex.

1st-person-singular-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 1st+sg ] ].

1st-person-dual-exclusive-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 1du_excl ] ].

1st-person-dual-inclusive-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 1du_incl ] ].

1st-person-plural-exclusive-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 1pl_excl ] ].

1st-person-plural-inclusive-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL.CAT.HEAD.PRON + ].

3rd-person-singular-polite-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 3sg+polite ] ].

3rd-person-plural-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM pl+3rd ] ].

3rd-person-dual-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL.CAT.HEAD.PRON + ].

2nd-person-plural-pronoun-noun-lex := PERSONAL-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PERNUM 2pl ] ].

PERSONAL-PRONOUNS-noun-lex := no-spr-noun-lex & CASE-POSITION-CLASS-rule-dtr & POSSESSIVE-POSITION-CLASS-rule-dtr &
  [ SYNSEM.LOCAL.CAT.HEAD.PRON +,
    INFLECTED [ CASE-POSITION-CLASS-FLAG -,
                POSSESSIVE-POSITION-CLASS-FLAG - ] ].

hearsay-discourse-marker-noun-lex := no-spr-noun-lex.

POSSESSIVE-PRONOUNS-noun-lex := no-spr-noun-lex & CASE-POSITION-CLASS-rule-dtr & POSSESSIVE-POSITION-CLASS-rule-dtr &
  [ INFLECTED [ CASE-POSITION-CLASS-FLAG -,
                POSSESSIVE-POSITION-CLASS-FLAG - ] ].

1st-Poss-Pronoun-noun-lex := POSSESSIVE-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM 1st ].

2nd-Poss-Pronoun-noun-lex := POSSESSIVE-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM 2nd ].

3rd-Poss-Pronoun-noun-lex := POSSESSIVE-PRONOUNS-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM 3rd ].

1de-poss-pronoun-noun-lex := 1st-Poss-Pronoun-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM 1du_excl ].

1pe-poss-pronoun-noun-lex := 1st-Poss-Pronoun-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM 1pl_excl ].

1di-poss-pronoun-noun-lex := 1st-Poss-Pronoun-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM 1du_incl ].

1pi-poss-pronoun-noun-lex := 1st-Poss-Pronoun-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM 1pl_incl ].

2du-poss-pronoun-noun-lex := 2nd-Poss-Pronoun-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM du ].

2pl-or-2polite-poss-pronoun-noun-lex := 2nd-Poss-Pronoun-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM pl+2polite ].

3du-poss-pronoun-noun-lex := 3rd-Poss-Pronoun-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM du ].

3pl-or-3polite-poss-pronoun-noun-lex := 3rd-Poss-Pronoun-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG.PERNUM pl+3polite ].

regarding-noun-noun-lex := no-spr-noun-lex.

;;; Verbs
verb-lex := non-mod-lex-item &
  [
    SYNSEM [ L-QUE -,
             LOCAL.CAT.HEAD verb &
                            [ NMZ - ],
             SUBORDINATED none ] ].

;;; parent type of the NP-predicating copulas (transitive verb-esque)
cop-np-pred-lex := norm-sem-lex-item & verb-lex & transitive-lex-item &
  [ SYNSEM [ LOCAL.CAT [ HEAD verb &
                            [ NMZ - ],
			 VAL [ SUBJ < #subj >,
			       COMPS < #comps > ] ],
	     L-QUE -,
             SUBORDINATED none ],
    ARG-ST < #subj & [ LOCAL.CAT cat-sat &
				 [ VAL.SPR < >,
				   HEAD noun &
				      [ CASE nom ] ] ],
             #comps &
             [ LOCAL.CAT cat-sat &
                         [ VAL.SPR < >,
			   HEAD noun &
                                [ CASE nom ] ] ] > ].

main-verb-lex := verb-lex & basic-verb-lex & basic-non-wh-word-lex & ASPECT-POSITION-CLASS-rule-dtr & GENITIVIZATION-or-NOMINALIZATION-rule-dtr & HEARSAY-Marking-Position-Class-rule-dtr & INTRANSITIVE-TENSE-ROLE-MARKING-or-TRANSITIVE-ROLE-MARKING-rule-dtr & MOOD-Position-Class-rule-dtr & NEGATION-position-class-rule-dtr & ROLE-POSITION-CLASS-rule-dtr & need-to-add-mood-rule-dtr &
[ INFLECTED [ ASPECT-POSITION-CLASS-FLAG -,
                ROLE-POSITION-CLASS-FLAG -,
                MOOD-POSITION-CLASS-FLAG -,
                1SG-PATIENT-FLAG na-or--,
                1DE-PATIENT-FLAG na-or--,
                1DI-PATIENT-FLAG na-or--,
                1PE-PATIENT-FLAG na-or--,
                1PI-PATIENT-FLAG na-or--,
                UNDERSPECIFIED-PATIENT-FLAG na-or--,
                3SG-PATIENT-FLAG na-or--,
                ADD-NEGATION-TO-VERB-FLAG na-or-- ],
  SYNSEM [ L-QUE -,
             LOCAL [ CAT [ HEAD.AUX -,
                           VAL [ SPEC < >,
                                 SUBJ < #subj > ] ],
                     CONT.HOOK.XARG #xarg ] ],
    ARG-ST.FIRST #subj &
                 [ LOCAL [ CAT cat-sat &
                               [ VAL [ SPR < >,
                                       COMPS < > ] ],
                           CONT.HOOK.INDEX #xarg ] ] ].

aux-lex := verb-lex & basic-icons-lex-item &
  [ SYNSEM [ LOCAL.CAT.HEAD.AUX +,
             L-QUE - ],
    INFLECTED [
                TRANSITIVE-VERB-FLAG na-or--,
                INTRANSITIVE-VERB-FLAG na-or-- ] ].

intransitive-verb-lex := main-verb-lex & intransitive-lex-item &
  [ SYNSEM.LOCAL.CAT.VAL.COMPS < >,
    ARG-ST < [ LOCAL.CAT.HEAD noun &
                              [ CASE real-case ] ] >,
    INFLECTED [ TRANSITIVE-VERB-FLAG na-or--,
                INTRANSITIVE-VERB-FLAG + ] ].

transitive-verb-lex := main-verb-lex & transitive-lex-item & Transitive-Non-Past-Agent-Role-Marker-or-Transitive-Past-Agent-Role-Marker-rule-dtr & Transitive-Patient-Role-Marker-rule-dtr &
  [ SYNSEM.LOCAL.CAT.VAL.COMPS < #comps >,
    ARG-ST < [ LOCAL.CAT.HEAD noun &
                              [ CASE real-case ] ],
             #comps &
             [ LOCAL.CAT cat-sat &
                         [ VAL [ SPR < >,
                                 COMPS < > ],
                           HEAD noun &
                                [ CASE real-case ] ] ] >,
    INFLECTED [
                TRANSITIVE-VERB-FLAG +,
                INTRANSITIVE-VERB-FLAG na-or-- ] ].

NOM-intransitive-verb-lex := intransitive-verb-lex &
  [ ARG-ST.FIRST.LOCAL.CAT.HEAD noun &
                                [ CASE NOM ] ].

ERG-DAT-transitive-verb-lex := transitive-verb-lex &
  [ ARG-ST < [ LOCAL.CAT.HEAD noun &
                              [ CASE ERG ] ],
             [ LOCAL.CAT.HEAD noun &
                              [ CASE DAT ] ] > ].

copula-np-pred-npst-verb-lex := cop-np-pred-lex & copula-npst-dtr &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.TENSE nonpast ].

copula-np-pred-pst-verb-lex := cop-np-pred-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.TENSE past ].

;;; Auxiliaries

subj-raise-aux := aux-lex & trans-first-arg-raising-lex-item &
  [ SYNSEM.LOCAL [ CAT.VAL [ SPEC < >,
                             COMPS < #comps >,
                             SUBJ < #subj &
                                    [ LOCAL.CAT.HEAD.CASE real-case ] > ],
                   CONT.HOOK.XARG #xarg ],
    ARG-ST < #subj &
             [ LOCAL [ CAT [ HEAD noun &
                                  [ CASE #case ],
                             VAL [ SUBJ < >,
                                   SPR < >,
                                   SPEC < >,
                                   COMPS < > ] ],
                       CONT.HOOK.INDEX #xarg ] ],
             #comps &
             [ LOCAL.CAT [ VAL [ SUBJ < unexpressed &
                                        [ LOCAL.CAT.HEAD.CASE #case ] >,
                                 COMPS < >,
                                 SPR < >,
                                 SPEC < > ],
                           HEAD verb ] ] > ].

; To keep the semantically empty ones from spinning on
; generation, require complement to be [AUX -].  The
; FORM feature might be enough in the starter grammars,
; but I don't want to rely on this.  Then again, [ AUX - ]
; might not be true.

