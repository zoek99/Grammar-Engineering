;;; Subordinators
;;;;;;;;;;;;;;;;;;;

adverb-subord-lex-item := no-rels-hcons-lex-item & basic-icons-lex-item &
  [ SYNSEM.LOCAL.CAT [ VAL [ SUBJ < >,
