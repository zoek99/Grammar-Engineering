  [ SYNSEM.LOCAL.CAT.VAL.COMPS < #comps >,
    ARG-ST < [ LOCAL.CAT.HEAD +np ],
             #comps &
             [ LOCAL.CAT cat-sat &
                         [ VAL [ SPR < >,
                                 COMPS < > ],
                           HEAD +np ] ] >,
    INFLECTED.TRANSITIVE-VERB-OR-VERB3-VERB-OR-VERB49-VERB-OR-VERB93-VERB-FLAG + ].

